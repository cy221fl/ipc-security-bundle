{
  "name": "cy/ipc-security-bundle",
  "type": "library",
  "description": "Fork of https://github.com/tburschka/ipc-security-bundle",
  "keywords": ["php"],
  "homepage": "https://github.com/cy221fl/ipc-security-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Torsten Burschka",
      "email": "torsten.burschka@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "ssh://git@bitbucket.ideaplexus.com:7999/ipc/corebundle.git"
    },
    {
      "type": "vcs",
      "url": "ssh://git@bitbucket.ideaplexus.com:7999/ipc/validatorbundle.git"
    }
  ],
  "require": {
    "php": ">= 7.0",
    "symfony/symfony": "^3.3",
    "doctrine/doctrine-bundle": "^1.6",
    "ipc/core-bundle": "^1.0",
    "ipc/validator-bundle": "^1.0"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "^3.3",
    "symfony/monolog-bundle": "@stable",
    "ipc/test-bundle": "@stable",
    "phpunit/phpunit": "^6.0",
    "pdepend/pdepend": "@stable",
    "phploc/phploc": "@stable",
    "phpmd/phpmd": "@stable",
    "sebastian/phpcpd": "@stable",
    "theseer/phpdox": "@stable",
    "squizlabs/php_codesniffer": "@stable"
  },
  "autoload": {
    "psr-4": {
      "IPC\\SecurityBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\IPC\\SecurityBundle\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "github-oauth": {
      "github.com": "0e82ae8022e3590063bfe6322d2cabceb4476c5a"
    }
  }
}